          "type": "string"
        },
        "cloud_area_fraction_medium": {
          "example": "%",
          "type": "string"
        },
        "dew_point_temperature": {
          "example": "C",
          "type": "string"
        },
        "fog_area_fraction": {
          "example": "%",
          "type": "string"
        },
        "precipitation_amount": {
          "example": "mm",
          "type": "string"
        },
        "precipitation_amount_max": {
          "example": "mm",
          "type": "string"
        },
        "precipitation_amount_min": {
          "example": "mm",
          "type": "string"
        },
        "probability_of_precipitation": {
          "example": "%",
          "type": "string"
        },
        "probability_of_thunder": {
          "example": "%",
          "type": "string"
        },
        "relative_humidity": {
          "example": "%",
          "type": "string"
        },
        "ultraviolet_index_clear_sky_max": {
          "example": "1",
          "type": "string"
        },
        "wind_from_direction": {
          "example": "degrees",
          "type": "string"
        },
        "wind_speed": {
          "example": "m/s",
          "type": "string"
        },
        "wind_speed_of_gust": {
          "example": "m/s",
          "type": "string"
        }
      },
      "type": "object"
    },
    "METJSONForecast": {
      "properties": {
        "geometry": {
          "$ref": "#/definitions/PointGeometry"
        },
        "properties": {
          "$ref": "#/definitions/Forecast"
        },
        "type": {
          "enum": [
            "Feature"
          ],
          "example": "Feature",
          "type": "string"
        }
      },
      "required": [
        "type",
        "geometry",
        "properties"
      ],
      "title": "GeoJSON Forecast Timeseries",
      "type": "object"
    },
    "PointGeometry": {
      "properties": {
        "coordinates": {
          "description": "[longitude, latitude, altitude]. All numbers in decimal.",
          "example": [
            60.5,
            11.59,
            1001
          ],
          "items": {
            "type": "number"
          },
          "minItems": 2,
          "type": "array"
        },
        "type": {
          "enum": [
            "Point"
          ],
          "type": "string"
        }
      },
      "required": [
        "type",
        "coordinates"
      ],
      "title": "GeoJSON point type",
      "type": "object"
    },
